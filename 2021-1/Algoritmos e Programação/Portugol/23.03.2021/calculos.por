programa
{
	funcao inicio()
	{
		inteiro a=2, b=5, c=10
		real calculo1, calculo2, calculo3
		
		calculo1 = a+b*c/a
		calculo2 = (a+b)*c/a
		calculo3 = (a+b*c)/a

		escreva("O primeiro cálculo tem como resultado ", calculo1, ".\n")
		escreva("O segundo cálculo tem como resultado ", calculo2, ".\n")
		escreva("O terceiro cálculo tem como resultado ", calculo3, ".\n")
	}
}
